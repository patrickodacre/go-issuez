{{define "content"}}

<h1>Features for Project - {{.Data.Name}}</h1>

<div>
    <a href="/projects/{{.Data.ID}}">Back to project</a>
</div>

<ul>
    {{range $k, $feature := .Data.Features}}
        <li><a href="/features/{{$feature.ID}}">{{$feature.Name}} - {{$feature.Description}}</a></li>
    {{end}}
</ul>
{{end}}

