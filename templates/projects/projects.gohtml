{{define "content_menu"}}
    <button type="button" class="btn btn-sm btn-primary">
        <span data-feather="file"></span>
        New Project
    </button>
{{end}}
{{define "content"}}
    <ul class="list-group">
        {{range $k, $project := .Data.Projects}}
            <li class="list-group-item"><a href="projects/{{$project.ID}}">{{$project.Name}} - {{$project.Description}}</a></li>
        {{end}}
    </ul>
{{end}}
